<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuPrincipal">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Lectura de Suministro"
        android:id="@+id/btLecturaSuministro" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Busqueda por Suministro"
        android:id="@+id/btBusquedaSuministro" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Modificar Lectura"
        android:id="@+id/btModificarLectura" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="CAMARA"
        android:id="@+id/btCamara" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Cargar Suministros"
        android:id="@+id/btCargarSuministro" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Envio. Suministro"
        android:id="@+id/btEnvioSuministro" />


</LinearLayout>
